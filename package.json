{
  "name": "angularlib-services",
  "version": "1.0.0",
  "description": "Example of how to create an angular module exporting services in order to be consumed as an npm package.",
  "main": "dist/bundles/angular-service-module.umd.js",
  "module": "dist/index.js",
  "es2015": "dist/index.js",
  "typings": "dist/index.d.ts",
  "devDependencies": {
    "@angular/compiler": "^4.3.0",
    "@angular/compiler-cli": "^4.3.0",
    "@angular/core": "^4.3.0",
    "@angular/http": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "@types/jasmine": "^2.5.53",
    "@types/node": "^8.0.13",
    "codelyzer": "^3.1.2",
    "jasmine-core": "^2.6.4",
    "rimraf": "^2.6.1",
    "rollup": "^0.45.2",
    "rxjs": "^5.4.2",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1",
    "uglify-js": "^3.0.25",
    "zone.js": "^0.8.13"
  },
  "peerDependencies": {
    "@angular/core": "^4.3.0",
    "@angular/http": "^4.3.0",
    "@angular/platform-browser": "^4.3.0",
    "zone.js": "^0.8.13"
  },
  "scripts": {
    "clean": "rimraf dist",
    "transpile": "ngc",
    "package": "rollup -c",
    "minify": "uglifyjs dist/bundles/angular-service-module.umd.js --screw-ie8 --compress --mangle --comments --output dist/bundles/angular-service.module.umd.min.js",
    "build": "npm run lint && npm run clean && npm run transpile && npm run package",
    "lint": "tslint 'src/**/*.ts'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/valudio/angular-service-module.git"
  },
  "keywords": [
    "angular",
    "modules",
    "services"
  ],
  "author": "Valudio <development@valudio.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/valudio/angular-service-module/issues"
  },
  "homepage": "https://github.com/valudio/angular-service-module#readme"
}
